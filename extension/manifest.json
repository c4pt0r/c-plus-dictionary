{
   "background": {
      "page": "background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "static/icon19.png",
      "default_popup": "page_action.html",
      "default_title": "C+ Dictionary"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content_css_min.css" ],
      "js": ["jquery.min.js", "content_js.js" ],
      "matches": [ "<all_urls>" ]
   } ],
   "content_security_policy": "script-src 'self' https://fanyi.youdao.com ; object-src 'self'",
   "description": "View definitions in both your language and English as you browse the web. Based on Google Dictionary by Google.",
   "icons": {
      "128": "static/icon128.png",
      "16": "static/icon16.png",
      "32": "static/icon32.png",
      "48": "static/icon48.png"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+A",
            "mac": "MacCtrl+Shift+A"
         }
      }
   },
   "manifest_version": 2,
   "name": "C+ Dictionary",
   "options_page": "options.html",
   "permissions": [ "tabs", "storage", "https://fanyi.youdao.com/*" ],
   "version": "0.1.1"
}
